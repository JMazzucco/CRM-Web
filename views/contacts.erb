<div class="content">
	<div class="contact-container">
		<h1>My Contacts</h1>
		<hr>
		<div class="all-contacts">
		  <% @@rolodex.contacts.each do |contact| %>
		    <a href="/contacts/<%= contact.id %>">
		    	<li><%= contact.first_name %> <%= contact.last_name %></li>
		    </a>
		  <% end %>
		</div>
		<a href="/contacts/new_contact">
			<div id="add-contact">
				<p>Add Contact</p>
			</div>
		</a>
	</div>
</div>